{
  "name": "ai-starter-with-rocky",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --turbopack",
    "build": "bun --bun next build --turbopack",
    "build:container": "bun --bun next build",
    "turbo": "bun --bun next dev --turbopack",
    "start": "bun --bun next start",
    "lint": "bun --bun next lint",
    "preview": "bun --bun next build --turbopack && bun --bun next start",
    "postinstall": "prisma generate",
    "email": "bun --bun email dev --dir emails --port 3333",
    "remove-content": "bun ./setup.mjs",
    "db:migrate": "bun --bun prisma migrate dev",
    "db:studio": "bun --bun prisma studio",
    "db:seed": "bun prisma/seed.ts",
    "db:reset": "bun --bun prisma migrate reset",
    "format": "bun --bun prettier --write .",
    "type-check": "bun --bun tsc --noEmit",
    "contentlayer:build": "bun --bun contentlayer2 build",
    "podman:up": "podman-compose -f podman/podman-compose.yml up -d",
    "podman:down": "podman-compose -f podman/podman-compose.yml down",
    "podman:build": "podman build -f podman/Containerfile -t ai-starter .",
    "podman:logs": "podman-compose -f podman/podman-compose.yml logs -f",
    "podman:logs:app": "podman-compose -f podman/podman-compose.yml logs -f app",
    "podman:logs:db": "podman-compose -f podman/podman-compose.yml logs -f db",
    "podman:ps": "podman-compose -f podman/podman-compose.yml ps",
    "podman:rebuild": "bun run podman:down && bun run podman:build && bun run podman:up"
  },
